curl -X POST -H "Content-Type: application/json" -d "{\"nome\": \"Prodotto\", \"prezzo\": 10 , \"marca\": \"marca\"}" http://localhost:8000/products

curl -X GET  http://localhost:8000/products

curl -X GET  http://localhost:8000/products/3

curl -X DELETE http://localhost:8000/products/3

curl -X PATCH -H "Content-Type: application/json" -d "{\"marca\": \"marca\"}" http://localhost:8000/products/3